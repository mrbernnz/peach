<form action = "/session" method = "POST">
  <label for = "username">
    Username
  </label>
  <input name = "username" type = "text">
  <label for = "password">
    Password:
  </label>
  <input name = "password" type = "password">
  <input name = "authenticity_token" value = "<%= form_authenticity_token() %>"
    type = "hidden">
<button>Sign In</button>
</form>
